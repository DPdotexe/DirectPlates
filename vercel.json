{
    "builds": [
      {
        "src": "client/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "client/**/*",
        "use": "@vercel/node"
      },
      {
        "src": "server/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/$1" },
      { "src": "/(.*)", "dest": "client/$1" }
    ]
  }
  