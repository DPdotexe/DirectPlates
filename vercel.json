{
    "builds": [
      {
        "src": "client/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "client/**/*",
        "use": "@vercel/node"
      },
      {
        "src": "server/**/*",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/api/(.*)", "dest": "/server/$1" },
      { "src": "/(.*)", "dest": "/client/$1" }
    ]
  }
  