{
    "builds": [
      { "src": "client/index.html", "use": "@vercel/static" },
      { "src": "client/**/*", "use": "@vercel/node" }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/client/$1" }
    ]
  }
  